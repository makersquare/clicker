<h1>Listing question_sets</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @question_sets.each do |question_set| %>
      <tr>
        <td><%= question_set.name %></td>
        <td><%= link_to 'Show', '/class_groups/#{:class_group_id}/question_sets/#{:id}' %></td>
        <td><%= link_to 'Edit', '/class_groups/#{:class_group_id}/question_sets/#{:id}/edit' %></td>
        <td><%= link_to 'Destroy', '/class_groups/#{:class_group_id}/question_sets/#{:id}', method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<h1>New question_set</h1>

<%= render 'form' %>
